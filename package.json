{
  "name": "react-hello",
  "version": "1.0.0",
  "description": "starter",
  "main": "src/backend/index.js",
  "repository": "https://github.com/patbonecrusher/react-starter.git",
  "scripts": {
    "start": "NODE_ENV=production node --harmony src/backend/index.js",
    "watch:server": "NODE_ENV=development nodemon | bunyan",
    "watch:client": "webpack-dev-server --progress --inline --config webpack.config.babel.js --hot --host 0.0.0.0",
    "watch": "npm run watch:server & npm run watch:client",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.babel.js"
  },
  "babel": {
    "plugins": [
      "transform-runtime",
      "transform-export-extensions",
      "transform-class-properties"
    ],
    "presets": [
      "es2015",
      "es2017",
      "react",
      "flow"
    ]
  },
  "keywords": [],
  "author": "pat laplante",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-es2017": "~6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "compression-webpack-plugin": "1.0.0",
    "css-loader": "0.28.7",
    "eslint": "4.6.1",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-react": "7.3.0",
    "html-webpack-plugin": "2.30.1",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "nodemon": "~1.12.0",
    "style-loader": "0.18.2",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1"
  },
  "dependencies": {
    "bunyan": "1.8.12",
    "ramda": "0.24.1",
    "react": "15.6.1",
    "react-addons-css-transition-group": "15.6.0",
    "react-bootstrap": "0.31.2",
    "react-css-transition": "0.7.4",
    "react-dom": "15.6.1",
    "react-modal": "2.3.1",
    "react-router-dom": "4.2.2",
    "koa": "2.3.0",
    "koa-body": "2.3.0",
    "koa-mount": "3.0.0",
    "koa-router": "7.2.1",
    "koa-sendfile": "2.0.0",
    "koa-socket": "4.4.0",
    "koa-static": "4.0.1"
  }
}
